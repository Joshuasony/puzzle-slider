{
  "name": "puzzle-slider",
  "version": "1.0.2",
  "description": "Small description for puzzle-slider goes here",
  "private": true,
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "ember test",
    "build-apk": "ember cdv:build --platform=android --environment=production --release -- --gradleArg=-PcdvVersionCode=10001",
    "prepare-apk": "npm run build-apk && npm run sign-apk && npm run optimize-apk",
    "sign-apk-x86": "jarsigner -verbose -sigalg SHA1withRSA -digestalg SHA1 -keystore puzzle-mobileapps.keystore ember-cordova/cordova/platforms/android/build/outputs/apk/android-x86-release-unsigned.apk puzzlemobileapps",
    "sign-apk-armv7": "jarsigner -verbose -sigalg SHA1withRSA -digestalg SHA1 -keystore puzzle-mobileapps.keystore ember-cordova/cordova/platforms/android/build/outputs/apk/android-armv7-release-unsigned.apk puzzlemobileapps",
    "sign-apk": "npm run sign-apk-x86 && npm run sign-apk-armv7",
    "optimize-apk-x86": "zipalign -v 4 ember-cordova/cordova/platforms/android/build/outputs/apk/android-x86-release-unsigned.apk android-x86-release-optimized.apk",
    "optimize-apk-armv7": "zipalign -v 4 ember-cordova/cordova/platforms/android/build/outputs/apk/android-armv7-release-unsigned.apk android-armv7-release-optimized.apk",
    "optimize-apk": "npm run optimize-apk-x86 && npm run optimize-apk-armv7"
  },
  "repository": "",
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "broccoli-asset-rev": "^2.4.5",
    "ember-ajax": "^2.4.1",
    "ember-cli": "2.10.0",
    "ember-cli-app-version": "^2.0.1",
    "ember-cli-autoprefixer": "^0.6.0",
    "ember-cli-babel": "^5.1.7",
    "ember-cli-code-coverage": "0.3.8",
    "ember-cli-dependency-checker": "^1.3.0",
    "ember-cli-eslint": "^3.0.0",
    "ember-cli-htmlbars": "^1.1.1",
    "ember-cli-htmlbars-inline-precompile": "^0.3.3",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-mirage": "0.2.4",
    "ember-cli-moment-shim": "^2.2.1",
    "ember-cli-qunit": "^3.0.1",
    "ember-cli-sass": "^5.6.0",
    "ember-cli-slick": "^1.0.14",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-test-loader": "^1.1.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-cordova": "0.3.13",
    "ember-export-application-global": "^1.0.5",
    "ember-gestures": "0.4.2",
    "ember-getowner-polyfill": "^1.0.1",
    "ember-hammertime": "^1.1.2",
    "ember-load-initializers": "^0.6.2",
    "ember-moment": "^6.1.0",
    "ember-notify": "^5.1.7",
    "ember-pad": "^1.2.2",
    "ember-paper": "1.0.0-alpha.8",
    "ember-reactive-helpers": "^0.4.0",
    "ember-resolver": "^2.0.3",
    "ember-responsive": "^2.0.0",
    "ember-route-action-helper": "^2.0.0",
    "ember-wormhole": "^0.5.1",
    "eslint-config-topaxi": "1.1.0",
    "liquid-fire": "0.26.5",
    "loader.js": "^4.0.10",
    "smoke-and-mirrors": "^0.6.2"
  }
}
